  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "ai": "npm:ai",
    "@vercel/ai-utils": "npm:@vercel/ai-utils",
    "@ai-sdk/groq": "npm:@ai-sdk/groq",
    "@ai-sdk/openai": "npm:@ai-sdk/openai",
    "@ai-sdk/provider": "npm:@ai-sdk/provider",
    "terminal-kit": "npm:terminal-kit"
  }
}
